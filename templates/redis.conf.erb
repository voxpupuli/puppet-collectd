# Generated by Puppet
LoadPlugin redis

<Plugin redis>
<% @nodes.each do |iname,ihash| -%>
  <Node "<%= iname %>">
<% if ihash['host'] -%>
    Host "<%= ihash['host'] %>"
<% end -%>
<% if ihash['port'] -%>
    Port "<%= ihash['port'] %>"
<% end -%>
<% if ihash['timeout'] -%>
    Timeout <%= ihash['timeout'] %>
<% end -%>
  </Node>
<% end -%>
</Plugin>
